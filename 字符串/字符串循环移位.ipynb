   "source": [
    "将字符串向右循环移动 k 位"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "94abb314",
   "metadata": {},
   "outputs": [],
   "source": [
    "def cycleshift(s,k):\n",
    "    s1 = s[-k:]\n",
    "    s2 = s[:-k]\n",
    "    return s1 + s2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "7f47d92a",
   "metadata": {},
   "outputs": [],
